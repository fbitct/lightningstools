<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="SimLinkup.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log.txt"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="0"/>
      <maximumFileSize value="1024KB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger : %message %newline %date
        [%thread] %level %logger : %exception{message} - %exception{source} -
        %exception{stacktrace} - %exception{targetsite} - %exception{helplink} -
        %exception{data} %newline %date [%thread] %level %logger : %stacktrace{3} %newline
        %date [%thread] %level %logger : %stacktracedetail{3} %newline "/>
      </layout>
    </appender>
    <root>
      <level value="ALL"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
  </log4net>
  <runtime>
    <legacyUnhandledExceptionPolicy enabled="true"/>
  </runtime>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
  </startup>
  <userSettings>
    <SimLinkup.Properties.Settings>
      <setting name="StartRunningWhenLaunched" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="LaunchAtWindowsStartup" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="MinimizeToSystemTray" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="MinimizeWhenStarted" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="UpgradeNeeded" serializeAs="String">
        <value>True</value>
      </setting>
    </SimLinkup.Properties.Settings>
  </userSettings>
</configuration>
